<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>console-%d{MMdd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern> -->
        </encoder>
    </appender>

    
    <logger name="com.spw.elife" level="debugger" />
    <logger name="org.springframework.web.servlet.DispatcherServlet" level="debugger" />
    <logger name="druid.sql" level="debugger" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
